{
  "manifest_version": 3,
  "name": "Citation Debug Step-by-Step",
  "version": "1.0.0",
  "description": "Debug content scripts step by step",
  
  "permissions": [
    "activeTab",
    "storage"
  ],
  
  "host_permissions": [
    "https://www.youtube.com/*"
  ],
  
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["utils/helpers.js"],
      "run_at": "document_end"
    }
  ]
} 